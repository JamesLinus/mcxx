#!/bin/bash

cat <<EOF
MCXX="@abs_top_builddir@/libtool --mode=execute @abs_top_builddir@/src/driver/mcxx --output-dir=@abs_top_builddir@/tests --profile=lt-mcxx --verbose"
test_CC=\${MCXX}
test_CXX=\${MCXX}
test_nolink=yes
EOF

[ ! -z "$TEST_SOURCE" ] && NAME=$(basename $TEST_SOURCE)

case "$NAME" in
    success*)
      ;;
    failure*)
      echo "test_compile_fail=yes"
      ;;
    *)
      ;;
esac
